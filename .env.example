# Configurações do Banco de Dados
# ================================

# Use 'yes' para Autenticação Windows, 'no' para SQL Auth
USE_TRUSTED_CONNECTION=yes

# Credenciais SQL (apenas se USE_TRUSTED_CONNECTION=no)
DB_USER=
DB_PASSWORD=

# Nome do banco de dados
DB_NAME=db_talhoes

# Servidor SQL Server
DB_HOST=localhost\SQLEXPRESS

# Google Maps API Key
GOOGLE_MAPS_API_KEY=YOUR_API_KEY_HERE

# Django Secret Key
DJANGO_SECRET_KEY=django-insecure-change-this-in-production-with-a-real-key
